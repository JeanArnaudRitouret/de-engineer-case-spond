# PostgreSQL Database Configuration
PGHOST=your_postgres_host
PGDATABASE=your_database_name
PGUSER=your_postgres_username
PGPASSWORD=your_postgres_password
PGPORT=5432

# Amazon Redshift Configuration
REDSHIFT_HOST=your_redshift_cluster_endpoint
REDSHIFT_DB=your_redshift_database
REDSHIFT_USER=your_redshift_username
REDSHIFT_PASSWORD=your_redshift_password
REDSHIFT_PORT=5439

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=your_aws_region

# AWS Database Migration Service (DMS) Configuration
TASK_ARN=arn:aws:dms:region:account:task/your_task_id
SOURCE_ENDPOINT_ARN=arn:aws:dms:region:account:endpoint:your_source_endpoint
TARGET_ENDPOINT_ARN=arn:aws:dms:region:account:endpoint:your_target_endpoint
REPLICATION_INSTANCE_ARN=arn:aws:dms:region:account:rep:your_replication_instance 